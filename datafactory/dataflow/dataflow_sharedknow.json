{
	"name": "dataflow_sharedknow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_target_parquet",
						"type": "DatasetReference"
					},
					"name": "RawDataBadges"
				},
				{
					"dataset": {
						"referenceName": "ds_target_parquet",
						"type": "DatasetReference"
					},
					"name": "RawDataComments"
				},
				{
					"dataset": {
						"referenceName": "ds_target_parquet",
						"type": "DatasetReference"
					},
					"name": "Rawlinktypes"
				},
				{
					"name": "Rawpostlinks"
				},
				{
					"name": "Rawposttypes"
				},
				{
					"name": "Rawusers"
				},
				{
					"name": "Rawvotes"
				},
				{
					"name": "Rawvotetypes"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          Id as string,",
				"          Name as string,",
				"          UserId as string,",
				"          Date as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> RawDataBadges",
				"source(output(",
				"          Id as string,",
				"          CreationDate as string,",
				"          PostId as string,",
				"          Score as string,",
				"          UserId as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> RawDataComments",
				"source(output(",
				"          Id as string,",
				"          Type as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> Rawlinktypes",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false) ~> Rawpostlinks",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false) ~> Rawposttypes",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false) ~> Rawusers",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false) ~> Rawvotes",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false) ~> Rawvotetypes"
			]
		}
	}
}